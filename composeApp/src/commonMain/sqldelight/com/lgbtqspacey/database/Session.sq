CREATE TABLE IF NOT EXISTS Session(
    id INTEGER PRIMARY KEY NOT NULL,
    token TEXT,
    expiration TEXT,
    userId TEXT,
    name TEXT,
    accessLevel TEXT,
    pronouns TEXT
);

INSERT OR REPLACE INTO Session(id)
VALUES (1);

insertSession:
UPDATE Session
SET token = ?, expiration = ?, userId = ?
WHERE id = 1;

insertUserInfo:
UPDATE Session
SET name = ?, accessLevel = ?, pronouns = ?
WHERE id = 1;

getSession:
SELECT * FROM Session
WHERE id = 1;

deleteSession:
DELETE FROM Session;
