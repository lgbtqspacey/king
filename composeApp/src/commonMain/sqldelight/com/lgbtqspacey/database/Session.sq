CREATE TABLE IF NOT EXISTS Session(
    token TEXT PRIMARY KEY,
    expiration TEXT,
    userId TEXT,
    name TEXT,
    accessLevel TEXT,
    pronouns TEXT
);

insertSession:
INSERT OR REPLACE INTO Session(token, expiration, userId)
VALUES (?, ?, ?);

insertUserInfo:
INSERT OR REPLACE INTO Session(name, accessLevel, pronouns)
VALUES (?, ?, ?);

getSession:
SELECT * FROM Session;

deleteSession:
DELETE FROM Session;
